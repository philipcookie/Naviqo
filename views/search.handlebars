
<ul class = "search_results">

	{{#each provider}}

	<div style ="height:10px">
	</div>

		<li> 

				<div class = "search_result">

					<div class = "selectDiv flex-center row">
					
						<div class = "col-md-3">
							<img class = "companyLogo" src = "/public/assets/company/{{this.SP_Firm_Logo}}">
							<p class = "address text-md-left"> {{this.SP_Firm_Address}}</br>
							{{this.SP_Firm_City}}, {{this.SP_Firm_City}} {{this.SP_Firm_Zip}}</p>
						</div>

						<div class = "col-md-6">
							<h2 class = "text-md-left"> {{this.SP_Firm_Name}}</h2>
							<p class = "text-md-left firmDescription"> {{this.SP_Firm_Overview}}</p>
						</div>

						<div class = "col-md-3">
							<fieldset class= "form-group">
							    <input type="checkbox" id="checkbox{{@index}}">
							    <label for="checkbox{{@index}}"></label>
							</fieldset>
						</div>
					
					</div>

				</div>

		</li>

	{{/each}}

</ul>
